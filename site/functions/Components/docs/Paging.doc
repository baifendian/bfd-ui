/**
 * @title 分页
 */
import { Component } from 'react'
import Paging from 'bfd/Paging'

class PagingDemo extends Component {

  constructor(props) {
    super()
  }

  render() {

    return (
      <Paging 
        currentPage={1}             
        totalPageNum={1000}
        pageSize={10}
        maxSeries={5}
        onPageChange={::this.onPageChange} >
      </Paging>
    )
  }

  onPageChange(page) {
    console.log(page)
  }
}

@component Paging